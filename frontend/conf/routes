# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

# An example controller showing a sample home page

GET         /                                                                         controllers.HomeController.index()

#GET     /                           controllers.HomeController.index

GET         /rest/:minlat/:minlon/:maxlat/:maxlon/:dateFrom/:dateTo/:eventType        controllers.RestController.getMarkers(minlat: Double, minlon: Double, maxlat: Double, maxlon: Double, dateFrom: String, dateTo: String, eventType: String)
GET         /assets/*file                                                             controllers.Assets.at(path="/public", file)
POST        /upload/                                                                  controllers.RecordConsumerController.saveRecord()
GET         /pictures/:file                                                           controllers.PicturesController.picture(file)
GET         /pictures_min/:file                                                       controllers.PicturesController.minPicture(file)
